{
  "name": "@adscore/nodejs-common",
  "version": "1.0.2",
  "description": "NodeJS client library for Adscore",
  "scripts": {
    "prepare-package": "rm -r -f ./dist && npm run generate-barrels && tsup && npm run copy-files && cd ./dist && npm pack",
    "publish-package": "cd ./dist && npm publish --access public",
    "copy-files": "cp package.json ./dist/ && cp readme.md ./dist/",
    "generate-barrels": "barrelsby --delete --directory ./src --location all",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Adscore",
  "homepage": "https://adscore.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Adscore/nodejs-common.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^18.19.0",
    "barrelsby": "^2.8.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "inet_xtoy": "^1.2.7"
  },
  "optionalDependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "libsodium": "^0.7.13",
    "libsodium-wrappers": "^0.7.13",
    "php-serialize": "^4.1.1"
  }
}