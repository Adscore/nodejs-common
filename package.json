    "@types/node": "^20.12.11",
    "barrelsby": "^2.8.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "inet_xtoy": "^1.2.7"
  },
  "optionalDependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
